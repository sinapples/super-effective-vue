{
  "name": "super-effective",
  "version": "1.0.0",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "bundlesize": "bundlesize",
    "firebase:deploy:ci": "firebase deploy --non-interactive --token $FIREBASE_TOKEN",
    "prettier:check": "prettier \"**/*.{vue,scss,sass,js,json}\" --list-different",
    "prettier:format-all": "prettier --write \"**/*.{vue,scss,sass,js,json}\"",
    "test:e2e:headless": "vue-cli-service test:e2e --headless"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@mdi/font": "5.9.55",
    "babel-preset-es2015": "^6.24.1",
    "core-js": "^3.9.1",
    "firebase": "^6.6.2",
    "is_js": "^0.9.0",
    "lodash": "^4.17.21",
    "pwacompat": "^2.0.17",
    "register-service-worker": "^1.7.2",
    "roboto-fontface": "*",
    "vue": "^2.6.12",
    "vue-head": "^2.2.0",
    "vue-router": "^3.5.1",
    "vuetify": "^2.4.0",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.168",
    "@vue/cli-plugin-babel": "^4.5.12",
    "@vue/cli-plugin-e2e-cypress": "4.1.2",
    "@vue/cli-plugin-eslint": "^4.5.12",
    "@vue/cli-plugin-pwa": "^3.12.1",
    "@vue/cli-plugin-unit-jest": "^4.5.12",
    "@vue/cli-service": "^4.5.12",
    "@vue/eslint-config-prettier": "^5.1.0",
    "@vue/test-utils": "^1.1.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^24.9.0",
    "babel-plugin-lodash": "^3.3.4",
    "bundlesize": "github:kefranabg/bundlesize",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^5.2.3",
    "firebase-tools": "^7.16.2",
    "node-sass": "^4.14.1",
    "prerender-spa-plugin": "^3.4.0",
    "sass-loader": "^8.0.2",
    "vue-cli-plugin-vuetify": "~2.3.1",
    "vue-template-compiler": "^2.6.12",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-merge": "^4.2.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/",
    "setupFilesAfterEnv": [
      "<rootDir>/tests/unit/setup.js"
    ]
  },
  "_id": "super-effective@1.0.0",
  "bundlesize": [
 
    {
      "path": "./dist/js/app*.js",
      "maxSize": "15 kB"
    }
  ],
  "engines": {
    "npm": ">=5.5.0",
    "node": ">=9.3.0"
  },
  "readme": "ERROR: No README data found!"
}
